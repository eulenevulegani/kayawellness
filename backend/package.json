    "compression": "^1.7.4",
    "dotenv": "^16.4.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-rate-limit": "^7.4.0",
    "express-validator": "^7.2.0",
    "winston": "^3.14.2",
    "ioredis": "^5.4.1",
    "socket.io": "^4.7.5",
    "resend": "^4.0.0",
    "bull": "^4.16.3",
    "multer": "^1.4.5-lts.1",
